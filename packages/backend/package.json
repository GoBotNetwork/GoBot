{
  "name": "@gobot/backend",
  "version": "0.1.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "start": "yarn build && node dist/index.js",
    "start:ts-node": "ts-node src/index.ts",
    "dev": "yarn clean && nodemon --exec yarn start",
    "dev:ts-node": "yarn clean && nodemon --exec yarn start:ts-node",
    "migrate": "typeorm migration:create -n GobotMigration"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "passport": "^0.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "@types/passport": "^1.0.7",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
