{
  "name": "bot",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "start": "yarn build && node dist/index.js",
    "start:ts-node": "ts-node src/index.ts",
    "dev": "yarn clean && nodemon --exec yarn start",
    "dev:ts-node": "yarn clean && nodemon --exec yarn start:ts-node",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0"
  }
}
